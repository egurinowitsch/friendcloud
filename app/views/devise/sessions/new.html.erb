
<div class="row">
	<div class="span4">
	<%= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name), html: {class: "well"}) do |f| %>
	 
	 	<fieldset>
	 		<legend>Log In</legend>
	 		<%= f.input :email %>
	 		<%= f.input :password %>

	  		

	  		<div><%= f.button :submit, "Sign in" %></div>
		</fieldset>
	<% end %>

	
</div>
	<div class=" wht_background4">
		<h4 class="header4">Signing in is easy and secure</h4>
	
	</div>
</div>
